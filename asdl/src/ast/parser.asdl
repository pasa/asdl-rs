root = (type* types)
type = SumType(typeId, constr* constructors, attrs?)
     | ProdType(typeId, field* fields)
constr = (constrId id, field* fields)
field = Required(typeId, id?)
      | Optional(typeId, id?)
      | Repeated(typeId, id?)
attrs = (field* fields)
typeId = (str id)
constrId = (str id)
id = (str id)
