root = (type* types, str* comments)
type = SumType(typeId, constr* constructors, attrs?, str* comments)
    | ProdType(typeId, field* fields, str* comments)
constr = (constrId id, field* fields, str* comments)
field = Required(typeId, id?)
    | Optional(typeId, id?)
    | Repeated(typeId, id?)
attrs = (field* fields)
typeId = (str id)
constrId = (str id)
id = (str id)
